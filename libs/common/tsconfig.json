{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "composite": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strict": false,
    "tsBuildInfoFile": "./build/.tsbuildinfo"
  }
}
